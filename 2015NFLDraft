-- who did the Steelers select	
select name, picked_by from picks where picked_by = "Pittsburgh";

-- what position was picked the most
select position, count(*) as total_picks from picks group by position
order by total_picks desc limit 3;

-- what team had the most picks
select picked_by, count(*) as num_picks from picks
group by picked_by
order by num_picks desc
limit 5;

-- list all the QB's taken
select * from picks where position = "QB";

-- which college had the most players drafted
select college, count(*) as total_players from picks
group by college
order by total_players desc
limit 10;

-- how many picks came from my school
select name, college from picks where college = "Penn State";

-- add pick number this time for another college
select name, college, pick_number from picks where college = "Duke";

-- Wanted practice with the case and like funtion
SELECT *,
  CASE
    WHEN college LIKE '%State' THEN 'State College'
    WHEN college LIKE '%University' THEN 'University'
    WHEN college LIKE '%Tech' THEN 'Technical College'
    ELSE 'Other'
  END AS college_category
FROM picks;

-- more practice with case function
SELECT
  name,
  CASE
    WHEN position = 'QB' THEN 'Quarterback'
    WHEN position = 'RB' THEN 'Running Back'
    WHEN position = 'WR' THEN 'Wide Receiver'
    WHEN position = 'TE' THEN 'Tight End'
    ELSE 'Other'
  END AS position_category
FROM picks limit 20;

-- is there a player that got drafted by either tampa bay or NE that is either a wr or qb
select * from picks where (picked_by = "Tampa Bay" or picked_by = "New England") and (position = "QB" or position = "WR");

select name, pick_number from picks where "position" = "TE" order by pick_number asc;

-- Not a deep draft for centers, QB's or S
select "position", count(*) as "amount_picked" from picks group by position order by "amount_picked" asc limit 3;

-- what pick number was the lions last pick
select picked_by, max(pick_number) as last_pick from picks where picked_by= "Detroit"
